{
    "multipart": [
        {   "when": { "distance": "1|2" },
            "apply": { "model": "dtbeachparty:block/palm_leaves/coconut_frond" }
        },
        {   "when": { "distance": "3" },
            "apply": { "model": "dtbeachparty:block/palm_leaves/coconut_core_top" }
        },
        {   "when": { "distance": "4" },
            "apply": { "model": "dtbeachparty:block/palm_leaves/coconut_core_bottom" }
        },
        {   "when": {"OR":[ { "direction": "0", "distance": "1|2" }, { "distance": "5|6|7" } ]},
            "apply": { "model": "beachparty:block/palm_leaves" }
        }
    ]
}